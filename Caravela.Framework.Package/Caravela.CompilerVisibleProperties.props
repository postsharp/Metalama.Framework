<Project>
    <PropertyGroup>
        <CaravelaCompilerVisiblePropertiesTargetsImported>True</CaravelaCompilerVisiblePropertiesTargetsImported>
    </PropertyGroup>
    <!-- Exports the properties to the compiler -->
    <ItemGroup>
        <CompilerVisibleProperty Include="DebugCaravela" />
        <CompilerVisibleProperty Include="DebugCaravelaAnalyzer" />
        <CompilerVisibleProperty Include="DebugCaravelaIde" />
        <CompilerVisibleProperty Include="CaravelaProjectId" />
        <CompilerVisibleProperty Include="CaravelaBuildTouchFile" />
        <CompilerVisibleProperty Include="CaravelaEnabled" />
        <CompilerVisibleProperty Include="CaravelaFormatOutput" />
        <CompilerVisibleProperty Include="CaravelaFormatCompileTimeCode" />
        <CompilerVisibleProperty Include="CaravelaUserCodeTrusted" />
        <CompilerVisibleProperty Include="CaravelaCompileTimeOnlyProject" />
        <CompilerVisibleProperty Include="AssemblyName" />
        <CompilerVisibleProperty Include="CaravelaDesignTimeEnabled" />
        <CompilerVisibleProperty Include="CaravelaAdditionalCompilationOutputDirectory" />
    </ItemGroup>

    <!-- More properties are already exported by Caravela.Compiler and don't need to be exported a second time. -->

</Project>