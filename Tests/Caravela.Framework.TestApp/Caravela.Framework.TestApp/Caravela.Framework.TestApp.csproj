<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <CaravelaEmitCompilerTransformedFiles>true</CaravelaEmitCompilerTransformedFiles>
    <CaravelaDebugTransformedCode>true</CaravelaDebugTransformedCode>
    <EnforceCodeStyleInBuild>False</EnforceCodeStyleInBuild>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Caravela.Framework.TestApp.Aspects\Caravela.Framework.TestApp.Aspects.csproj" />
    <PackageReference Include="Caravela.Framework" Version="$(CaravelaVersion)" />
  </ItemGroup>
</Project>
