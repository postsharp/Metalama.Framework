<Project InitialTargets="ValidateVersion">

	<PropertyGroup>
		<DisableCodeQuality>True</DisableCodeQuality>
	</PropertyGroup>
	
  <!-- Imports Directory.Build.props of the upper directory. Use this code in any non-root Directory.Build.* even if the file does not exit. -->
  <Import Project="$([MSBuild]::GetPathOfFileAbove('Directory.Build.props', '$(MSBuildThisFileDirectory)../'))" Condition="Exists('$([MSBuild]::GetPathOfFileAbove(`Directory.Build.props`, `$(MSBuildThisFileDirectory)../`))')" />


  <PropertyGroup>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <DebugCaravelaAnalyzer>True</DebugCaravelaAnalyzer>
	 <DebugCaravelaIde>False</DebugCaravelaIde>
    <!-- Overwrite from the parent directory -->
    
  </PropertyGroup>


  <!-- Versions of dependencies -->
  <Target Name="ValidateVersion">
    <Error Text="The ProductVersion property is not defined. Check that Directory.Build.props have been properly imported." Condition="'$(CaravelaVersion)'==''"/>
  </Target>

</Project>