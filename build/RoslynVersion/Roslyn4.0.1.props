<Project>
    <PropertyGroup>
        <ThisRoslynVersion>4.0.1</ThisRoslynVersion>
        <!-- ThisRoslynVersionProjectSuffix should be empty for the latest version, otherwise it should be a dotted followed by ThisRoslynVersion -->
        <ThisRoslynVersionProjectSuffix></ThisRoslynVersionProjectSuffix>
        <DefineConstants>$(DefineConstants);ROSLYN_4_0_1</DefineConstants>
        <!-- DisableCodeQuality should be True except for the latest version -->
        <DisableCodeQuality></DisableCodeQuality>
    </PropertyGroup>
</Project>